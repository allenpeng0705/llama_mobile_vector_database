# Llama Mobile VD Build Configuration
# This file contains all configuration variables for building the project
# The pre-build scripts will try to auto-detect values and update this file

[core]
# Core library build configuration
CMAKE_PATH=
BUILD_TYPE=Release
NUM_CORES=
BUILD_DIR=build-lib

[ios]
# iOS build configuration
XCODE_DEVELOPER_DIR=
DEPLOYMENT_TARGET=13.0
ARCHITECTURES=arm64 x86_64

[android]
# Android build configuration
ANDROID_HOME=
ANDROID_NDK_ROOT=
NDK_VERSION=29.0.14206865
ANDROID_PLATFORM=android-24
ARCHITECTURES=arm64-v8a x86_64 armeabi-v7a x86
JAVA_HOME=

[flutter]
# Flutter SDK build configuration
FLUTTER_PATH=

[react-native]
# React Native SDK build configuration
REACT_NATIVE_PATH=
NODE_PATH=

[capacitor]
# Capacitor plugin build configuration
CAPACITOR_PATH=
